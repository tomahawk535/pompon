</p>\r\n          <p  v-else class=\"price\">you price</p>\r\n\r\n        </Tab>\r\n          <button\r\n          @click=\"addOrder()\"> wont more</button>\r\n         <button\r\n          @click=\"removeOrder()\">Want less </button>\r\n        <Tab\r\n          :isSelected=\"selected === 'Шаг 2: Доставка помпонов'\" >\r\n          <Deliveryinfo></Deliveryinfo>\r\n\r\n      </Tab>\r\n      <Tab\r\n          :isSelected=\"selected === 'Шаг 3: Подтверждение заказа'\" >\r\n          <p>Some test text3</p>\r\n      </Tab>\r\n    </Tabs>\r\n    <p>selected option {{selectedQ}}</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Tabs from \"@/components/Tabs\";\r\nimport Tab from \"@/components/Tab\";\r\nimport SetColor from \"@/components/SetCollor\";\r\nimport Deliveryinfo from \"@/components/Deliveryinfo\";\r\nimport SelectSize from \"@/components/SelectSize\";\r\nimport SelectQuantuty from \"@/components/SelectQuantuty\";\r\nexport default {\r\nname: \"OrderInfo\",\r\n  components:{\r\n    SelectQuantuty,\r\n    SelectSize,\r\n    Tabs,\r\n    Tab,\r\n    SetColor,\r\n    Deliveryinfo,\r\n  },\r\n  data() {\r\n    return {\r\n      selected: 'Шаг 1: Выбор помпонов',\r\n      tabs: [\r\n        'Шаг 1: Выбор помпонов',\r\n        'Шаг 2: Доставка помпонов',\r\n        'Шаг 3: Подтверждение заказа',\r\n      ],\r\n      // order\r\n      orders: [\r\n        1\r\n      ],\r\n      // option for set color\r\n      pompons: [\r\n        {\r\n          'name': 'Black',\r\n          'img': 'bk.png'\r\n        },\r\n        {\r\n          'name': 'Blue',\r\n          'img': 'bl.png'\r\n        },\r\n        {\r\n          'name': 'Ceris',\r\n          'img': 'cs.png'\r\n        },\r\n        {\r\n          'name': 'Green',\r\n          'img': 'gn.png'\r\n        },\r\n        {\r\n          'name': 'Ivory',\r\n          'img': 'iv.png'\r\n        },\r\n        {\r\n          'name': 'light Blue',\r\n          'img': 'lb.png'\r\n        },\r\n        {\r\n          'name': 'Lavander',\r\n          'img': 'ld.png'\r\n        },\r\n        {\r\n          'name': 'Blue',\r\n          'img': 'bl.png'\r\n        },\r\n        {\r\n          'name': 'Lilac',\r\n          'img': 'll.png'\r\n        },\r\n        {\r\n          'name': 'Mint',\r\n          'img': 'mt.png'\r\n        },\r\n        {\r\n          'name': 'Orange',\r\n          'img': 'og.png'\r\n        },\r\n        {\r\n          'name': 'Peach',\r\n          'img': 'ph.png'\r\n        },\r\n        {\r\n          'name': 'Pink',\r\n          'img': 'pk.png'\r\n        },\r\n        {\r\n          'name': 'Purple',\r\n          'img': 'pr.png'\r\n        },\r\n        {\r\n          'name': 'Red',\r\n          'img': 'rd.png'\r\n        },\r\n        {\r\n          'name': 'White',\r\n          'img': 'wh.png'\r\n        },\r\n        {\r\n          'name': 'Yellow',\r\n          'img': 'yw.png'\r\n        },\r\n      ],\r\n      selectC: \"Color\",\r\n      // option fo selectSize\r\n      selectSize: [\r\n        {name :'15 см', value: 1, price: 10},\r\n        {name :'25 см', value: 2, price: 20},\r\n        {name :'35 см', value: 3, price: 30},\r\n        {name :'45 см', value: 4, price: 40},\r\n      ],\r\n      selectedS: 'Размер',\r\n      // option for select quantity\r\n      quantity: [\r\n        { name: 10, value: 1},\r\n        { name: \"15\", value: 2},\r\n        { name: \"20\", value: 3},\r\n        { name: \"25\", value: 4},\r\n        { name: \"30\", value: 5},\r\n        { name: \"35\", value: 6},\r\n        { name: \"40\", value: 7},\r\n        { name: \"45\", value: 8},\r\n        { name: \"50\", value: 9},\r\n        { name: \"55\", value: 10},\r\n        { name: \"60\", value: 11},\r\n        { name: \"65\", value: 12},\r\n        { name: \"70\", value: 13},\r\n        { name: \"75\", value: 14},\r\n        { name: \"80\", value: 15},\r\n        { name: \"85\", value: 16},\r\n        { name: \"90\", value: 17},\r\n        { name: \"95\", value: 18},\r\n        { name: \"100\", value: 19},\r\n      ],\r\n      selectedQ:'Количество',\r\n\r\n\r\n      sum: \"\"\r\n    }\r\n  },\r\n  methods: {\r\n    setSelected(tab) {\r\n      this.selected = tab;\r\n    },\r\n    selectOption(selectedSize) {\r\n      this.selectedS = selectedSize.name\r\n      },\r\n    selectColor (selectedColor) {\r\n      this.selectC = selectedColor.name\r\n    },\r\n    selectQuantity(selectQuantity) {\r\n      this.selectedQ = selectQuantity.name\r\n    },\r\n    // add new to order arr\r\n    addOrder(){\r\n      this.orders.push()\r\n    },\r\n    removeOrder(){\r\n      this.orders.pop()\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .tebb{\r\n   display: flex;\r\n    justify-content: space-around;\r\n    flex-wrap: wrap;\r\n }\r\n  .price {\r\n    background-color: #6fcaea;\r\n  }\r\n</style>"],"sourceRoot":"src/components"}]}